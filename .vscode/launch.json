    "version": "0.2.0",
    "configurations": [
        {
            "name": "Emedgene debugger",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Run Pytest",
            "type": "debugpy",
            "request": "launch",
            "module": "pytest",
            "args": [
                "--cache-clear",
                "-v"
            ],
            "console": "integratedTerminal"
        }
    ]
}